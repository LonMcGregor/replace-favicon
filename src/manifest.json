{
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "contentscript.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_idle"
   } ],
   "description": "Provides a random jdenticon for websites missing a favicon.",
   "icons": {
      "128": "128.png",
      "48": "48.png"
   },
   "manifest_version": 2,
   "name": "Replace Missing Favicon",
   "background": {
      "scripts": [ "bg.js" ],
      "persistent": false
  },
   "version": "0.5.1"
}
